.navigation-item {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: var(--unitSpacing);
  height: 140px;
  font-style: normal;
  font-weight: bold;
  font-size: 24px;
  line-height: 30px;
  text-align: center;
  color: var(--white);
  cursor: pointer;

  &:hover {
    background: var(--blackA1);
  }
}
